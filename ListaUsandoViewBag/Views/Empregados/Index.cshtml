@{
    ViewBag.Title = "Empregados";

    //Inserindo essa página na pasta Layout, ou seja, essa paginá se torna o body que será renderizado no RenderBody do Layout;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>@ViewBag.Msg</h1>
<div class="wrapper">
    <div class="container">
        <table class="table">
            <thead>
                <tr>
                   <th scope="col">Id</th>
                   <th scope="col">Nome</th>
                   <th scope="col">Cidade</th>
                   <th scope="col">País</th>
                   <th scope="col">Endereço</th>
                   <th colspan="2">Ações</th>
                </tr>
            </thead> 
            @foreach (var item in ViewBag.model)
            {
               <tbody>
                  <tr>
                     <th scope="row">@item.Id</th>
                     <td>@item.Nome</td>
                     <td>@item.Cidade</td>
                     <td>@item.Pais</td>
                     <td>@item.Endereco</td>    
                     <td><a class="btn btn-primary">Editar</a></td>    
                     <td><a class="btn btn-danger">Deletar</a></td>    
                  </tr>
               </tbody>
            }
        </table>
    </div>
</div>